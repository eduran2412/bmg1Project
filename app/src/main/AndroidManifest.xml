<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.erenduran.foodsociety">

     <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    // adamın kendi storage ına gidip(hafızasına gidip) galerisinden resim seçeceğiz (izin isteme bu)


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodSociety">
        <activity android:name=".UploadActivity"></activity>
        <activity android:name=".FeedActivity" />
        <activity android:name=".SignUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>